<!-- ğŸ” Router (FINAL page): if the combo is one of the 8 PQ combos, send them back to pq-tbc-english -->
<script>
(function routeFromFINAL(){
  const PQ_SET = new Set([
    "rhino+love_bird","rhino+owl","rhino+sheep",
    "owl+love_bird","owl+sheep",
    "love_bird+rhino","love_bird+owl","love_bird+sheep"
  ]);
  const p = new URLSearchParams(location.search);
  const scores = {
    sheep:     parseInt(p.get("sheep"))     || 0,
    love_bird: parseInt(p.get("love_bird")) || 0,
    owl:       parseInt(p.get("owl"))       || 0,
    rhino:     parseInt(p.get("rhino"))     || 0,
  };
  if (!(scores.sheep || scores.love_bird || scores.owl || scores.rhino)) return;
  const order = ["rhino","owl","love_bird","sheep"];
  const ranked = Object.keys(scores).sort((a,b)=>{
    if (scores[b] === scores[a]) return order.indexOf(a) - order.indexOf(b);
    return scores[b] - scores[a];
  });
  const key = `${ranked[0]}+${ranked[1]}`;
  const PQ_PATH = "/pages/pq-tbc-english";
  if (PQ_SET.has(key)) location.replace(`${PQ_PATH}${location.search}`);
})();
</script>

