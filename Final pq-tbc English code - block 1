<!-- üö¶ Router to send new combos to the new page -->
<script>
(function routeFromPQ(){
  const FINAL_SET = new Set(["owl+rhino", "sheep+rhino", "sheep+owl", "sheep+love_bird"]);

  const params = new URLSearchParams(location.search);
  const scores = {
    sheep: parseInt(params.get("sheep")) || 0,
    love_bird: parseInt(params.get("love_bird")) || 0,
    owl: parseInt(params.get("owl")) || 0,
    rhino: parseInt(params.get("rhino")) || 0,
  };

  if (!(scores.sheep || scores.love_bird || scores.owl || scores.rhino)) return;

  const order = ["rhino", "owl", "love_bird", "sheep"];
  const ranked = Object.keys(scores).sort((a,b) => {
    if (scores[b] === scores[a]) return order.indexOf(a) - order.indexOf(b);
    return scores[b] - scores[a];
  });
  const key = `${ranked[0]}+${ranked[1]}`;

  const FINAL_PATH = "/pages/final-30-day-plan"; // change if handle differs
  if (FINAL_SET.has(key)) location.replace(`${FINAL_PATH}${location.search}`);
})();
</script>

<script>
document.addEventListener("DOMContentLoaded", function () {
  const params = new URLSearchParams(window.location.search);
  const scores = {
    sheep: parseInt(params.get("sheep")) || 0,
    love_bird: parseInt(params.get("love_bird")) || 0,
    owl: parseInt(params.get("owl")) || 0,
    rhino: parseInt(params.get("rhino")) || 0,
  };
  const firstName = params.get("first_name");

  // Bars + percents
  const total = scores.sheep + scores.love_bird + scores.owl + scores.rhino || 0;
  const pct = k => total ? Math.round((scores[k] / total) * 100) : 0;
  document.getElementById("score-sheep").textContent = pct("sheep") + "%";
  document.getElementById("bar-sheep").style.width = pct("sheep") + "%";
  document.getElementById("score-love_bird").textContent = pct("love_bird") + "%";
  document.getElementById("bar-love_bird").style.width = pct("love_bird") + "%";
  document.getElementById("score-owl").textContent = pct("owl") + "%";
  document.getElementById("bar-owl").style.width = pct("owl") + "%";
  document.getElementById("score-rhino").textContent = pct("rhino") + "%";
  document.getElementById("bar-rhino").style.width = pct("rhino") + "%";

  if (firstName) {
    document.getElementById("greeting").innerHTML =
      `Hey <span class="highlight-name" style="font-weight:800;background:linear-gradient(to right,#1b5fa8,#a0bede);padding:2px 6px;border-radius:6px;color:#fff;">${firstName}</span>, here‚Äôs your 30-day blueprint üëá`;
  }

  // Determine PRIMARY and SECONDARY (tie-break order keeps behavior stable)
  const order = ["rhino", "owl", "love_bird", "sheep"]; // tie-breaker priority
  const ranked = Object.keys(scores).sort((a,b) => {
    if (scores[b] === scores[a]) return order.indexOf(a) - order.indexOf(b);
    return scores[b] - scores[a];
  });
  const primary = ranked[0];
  const secondary = ranked[1];

  // Titles by animal
  const names = { sheep:"Sheep", love_bird:"Lovebird", owl:"Owl", rhino:"Rhino" };
  
document.getElementById("result-title").innerHTML =
  `Your Personality Combination: 
   <span class="highlight-name">${names[primary].toUpperCase()}</span> + 
   <span class="highlight-name">${names[secondary].toUpperCase()}</span>`;
 document.getElementById("result-description").textContent =
    "Strong match found. Your plan blends your first move with your correction move for consistent results.";

  // üîª TWO-ANIMAL CONTENT (your exact copy, lightly formatted) -----------------
